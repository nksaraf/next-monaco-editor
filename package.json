{
  "name": "next-monaco-editor",
  "author": "Nikhil Saraf",
  "version": "0.0.1-beta.21",
  "license": "MIT",
  "main": "dist/next-monaco-editor/index.js",
  "typings": "dist/next-monaco-editor/esm/index.d.ts",
  "module": "dist/next-monaco-editor/esm/index.js",
  "files": [
    "dist",
    "next-plugin",
    "api",
    "worker",
    "plugins"
  ],
  "scripts": {
    "start": "tsdx watch",
    "build": "npm-run-all --parallel build:*",
    "prepublishOnly": "yarn build",
    "dev": "next",
    "build:main": "tsdx build --name next-monaco-editor --tsconfig ./tsconfig.tsdx.json && yarn create:entry next-monaco-editor",
    "build:api": "tsdx build -i ./src/api/index.ts --tsconfig ./tsconfig.tsdx.json --name api && yarn create:entry api",
    "build:plugins": "tsdx build -i ./src/plugins/index.ts --tsconfig ./tsconfig.tsdx.json --name plugins && yarn create:entry plugins && yarn deno scripts/copy-workers.ts",
    "build:worker": "tsdx build -i ./src/worker/index.ts --tsconfig ./tsconfig.tsdx.json --name worker && yarn create:entry worker",
    "create:entry": "yarn deno scripts/entry.ts",
    "deno": "deno --allow-all"
  },
  "peerDependencies": {
    "next": "*",
    "react": ">=16"
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/node": "^7.8.7",
    "@types/color": "^3.0.1",
    "@types/dedent": "^0.7.0",
    "@types/jest": "^25.2.1",
    "@types/react": "^16.9.32",
    "@types/react-dom": "^16.9.6",
    "color": "^3.1.2",
    "dedent": "^0.7.0",
    "esm": "^3.2.25",
    "graphiql-explorer": "^0.5.1",
    "graphql": "^15.0.0",
    "graphql-config": "^3.0.0-rc.2",
    "husky": "^4.2.3",
    "magic-components": "^0.0.2-beta.7",
    "match-sorter": "^4.1.0",
    "monaco-themes": "^0.3.3",
    "next": "^9.3.4",
    "npm-run-all": "^4.1.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "ts-morph": "^7.0.1",
    "tsdx": "^0.13.1",
    "tslib": "^1.11.1",
    "typescript": "^3.8.3",
    "vscode-languageserver-types": "^3.15.1"
  },
  "dependencies": {
    "@graphql-toolkit/common": "^0.10.3",
    "babel-loader": "^8.1.0",
    "monaco-editor": "^0.20.0",
    "monaco-editor-webpack-plugin": "^1.9.0",
    "monaco-languages": "^1.10.0",
    "next-transpile-modules": "^3.2.0",
    "prettier": "^2.0.4",
    "react-spectrum": "^1.1.7",
    "worker-loader": "^2.0.0",
    "workerize-loader": "^1.1.0"
  }
}
