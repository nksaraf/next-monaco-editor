    "codesandbox-import-utils": "^2.1.14",
    "create-hook-context": "^0.0.3",
    "faunadb": "^2.13.1",
    "graphql-ast-types": "^1.0.2",
    "idb-keyval": "^3.2.0",
    "json-to-pretty-yaml": "^1.2.2",
    "lodash.debounce": "^4.0.8",
    "monaco-editor": "^0.20.0",
    "monaco-editor-webpack-plugin": "^1.9.0",
    "monaco-languages": "^1.10.0",
    "next-transpile-modules": "^3.2.0",
    "react-modal": "^3.11.2",
    "react-query": "^1.5.5",
    "react-spectrum": "^1.1.7",
    "react-split": "^2.0.7",
    "react-tippy": "^1.3.4",
    "react-ui": "^1.0.0-beta.24",
    "rxjs": "^6.5.5",
    "scope-eval": "^1.0.0",
